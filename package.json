    "@rollup/plugin-typescript": "^11.1.0",
    "@types/electron": "^1.6.10",
    "@types/node": "^14.14.2",
    "obsidian": "^1.2.8",
    "rollup": "^3.21.6",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "es-mime-types": "^0.1.4",
